{
  "name": "docker-demo",
  "version": "1.0.0",
  "description": "My Moleculer microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "cli": "moleculer connect ",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "MoleculerJS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moleculerjs/docker-demo.git"
  },
  "devDependencies": {
    "eslint": "^6.0.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "moleculer-repl": "^0.5.7"
  },
  "dependencies": {
    "ioredis": "4.10.0",
    "moleculer": "^0.13.9",
    "moleculer-web": "^0.8.5",
    "nats": "1.3.0"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
