{
  "name": "docker-demo",
  "version": "1.3.1",
  "description": "My Moleculer microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner --repl",
    "cli": "moleculer connect ",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "MoleculerJS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moleculerjs/docker-demo.git"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "moleculer-repl": "^0.6.2"
  },
  "dependencies": {
    "ioredis": "^4.14.1",
    "mkdirp": "^0.5.1",
    "moleculer": "^0.13.12",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.6",
    "moleculer-web": "^0.8.5",
    "mongodb": "^3.4.1",
    "nats": "^1.3.2"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
